<Window x:Class="Lab12.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab12"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <StackPanel>
            <TextBox Text="Введите строку" x:Name="inptText"/>
            <Button x:Name="btnAdd" Click="btnAdd_Click">ADD</Button>
            <ComboBox x:Name="comboBox" SelectionChanged="comboBox_SelectionChanged"/>
            <ListBox x:Name="listBox" Height="200"/>
            <Button x:Name="btnRun" Click="btnRun_Click">RUN</Button>
            <Button x:Name="btnClear" Click="btnClear_Click">Clear</Button>
            <ListBox x:Name="listBoxRun" Height="100"/>
        </StackPanel>
    </Grid>
</Window>
