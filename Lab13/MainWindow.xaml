<Window x:Class="Lab13.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab13"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button x:Name="btnPLay" Width="200" Height="50" Click="btnPLay_Click">Play</Button>
            <Slider x:Name="slSLider"  Margin="0,20,0,0" Value="0" Minimum="1" Maximum="101" SelectionStart="0" Height="50" ></Slider>
            <ProgressBar x:Name="barProgress" Minimum="1" Maximum="101" Margin="0,20,0,0" Height="50"></ProgressBar>
            <TextBlock x:Name="textResult"  Height="50"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
